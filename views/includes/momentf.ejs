<script>
        function renderDate(value){
            
            var formattedTime = moment(value).locale('fr').format('MMMM Do YYYY, h:mm');
            
        return formattedTime;
        }
        function renderDate2(value){
            
            var formattedTime = moment(value).locale('fr').fromNow();
            
        return formattedTime;
        }
        </script>